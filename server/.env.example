# Google OAuth
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com

# JWT Secret (generate: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
JWT_SECRET=change-me-to-random-secret

# Server port (dev: 3000, prod: 8002)
PORT=3000

# Database
DATABASE_PATH=../data/site.db

# CORS origins (comma-separated)
ALLOWED_ORIGINS=http://localhost:8000

# Instagram data path
INSTAGRAM_DATA_PATH=../data/instagram_data_local.json
